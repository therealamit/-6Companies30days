class Solution {
    public String longestPrefix(String s) {
        int n=s.length();
        int[] lps=new int[n];
        lps[0]=0;
        int len=0;
        int i=1;
        while(i<n){
            if(s.charAt(i)==s.charAt(len)){
                lps[i++]=++len;
            }
            else{
                if(len==0)
                    lps[i++]=0;
                else
                    len=lps[len-1];
            }
        }
        return s.substring(0,lps[n-1]);
    }
}
